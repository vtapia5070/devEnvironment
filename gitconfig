# ~/.gitconfig

[user]
  name = victoria tapia
  email = victoria_tapia@discovery.com

[color]
  ui = true

[core]
  autocrlf = input
  editor = subl -n -w
  ignorecase = false
  safecrlf = true


[alias]
  br = branch
  ch = checkout
  com = commit
  com-undo = reset --soft HEAD^
  push-master = push origin master
  push-staging = push origin staging
  st = status
